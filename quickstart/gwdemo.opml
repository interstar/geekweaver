			<outline text="&amp;about :tpl" created="Sat, 27 Jul 2013 18:42:35 GMT">
				<outline text="title: About" created="Sat, 27 Jul 2013 18:43:08 GMT"/>
				<outline text="main :" created="Sat, 27 Jul 2013 18:43:16 GMT">
					<outline text=".&lt;p&gt;" created="Sat, 27 Jul 2013 18:43:18 GMT">
						<outline text="This is a second page. See how one outline compiles into multiple pages?" created="Sat, 27 Jul 2013 18:43:25 GMT"/>
						</outline>
					<outline text=".&lt;p&gt;" created="Sat, 27 Jul 2013 18:45:10 GMT">
						<outline text="This site was made with the following technologies : " created="Sat, 27 Jul 2013 18:45:13 GMT">
							<outline text="*" created="Sat, 27 Jul 2013 18:45:37 GMT">
								<outline text="&gt; GeekWeaver &gt; http://geekweaver.com" created="Sat, 27 Jul 2013 18:45:53 GMT"/>
								<outline text="&gt;  Fargo (OPML editor) &gt; http://fargo.io " created="Sat, 27 Jul 2013 18:45:35 GMT"/>
								<outline text="&gt; Python &gt; http://python.org " created="Sat, 27 Jul 2013 18:45:23 GMT"/>
								</outline>
							</outline>
						</outline>
					<outline text=".&lt;h3&gt;" created="Sat, 27 Jul 2013 22:06:35 GMT">
						<outline text="Learn More" created="Sat, 27 Jul 2013 22:06:52 GMT"/>
						</outline>
					<outline text=".&lt;p&gt;" created="Sat, 27 Jul 2013 18:43:42 GMT">
						<outline text="*" created="Sat, 27 Jul 2013 20:19:22 GMT">
							<outline text="&gt; See the source of this site &gt; http://reader.smallpicture.com/?opmlurl=https%3A%2F%2Fdl.dropbox.com%2Fs%2Fo90jg9ac55y7b74%2Fgwdemo.opml " created="Sat, 27 Jul 2013 20:19:32 GMT"/>
							<outline text="&gt; Get the source as a basis for your own GeekWeaver site. &gt; https://dl.dropbox.com/s/o90jg9ac55y7b74/gwdemo.opml" created="Sat, 27 Jul 2013 19:29:38 GMT"/>
							</outline>
